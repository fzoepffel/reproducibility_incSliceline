addedX10 = X[nrow(X) - n + 1 : nrow(X),  ]
addedE10 = e[nrow(e) - n + 1 : nrow(e), ]


X = oldX;
e = oldE;
sigma = ceil(0.15 * nrow(addedX1))

start = time();
[S,C,D] = slicefinder(X=X, e=e, k=10, alpha=0.95, minSup=sigma, tpBlksz=16, verbose=TRUE);
end = time()

timeTaken = (((end-start) / 1000000 - 1500))

write(timeTaken, "results/Experiment2_timesSF1.dat", format="csv")


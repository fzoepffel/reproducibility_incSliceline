X = read($1);
e = read($2);

X = X[, 1:8];

n = ceil(nrow(X)/100);

oldX = X[1:nrow(X) - n * 10 ,  1:8]
addedX1 = X[nrow(X) - n*10 + 1 : nrow(X) - n*9,  ]
oldE = e[1:nrow(e) - n * 10 ,]
addedE1 = e[nrow(e) - n *10 + 1 : nrow(e) - n*9, ]

addedX2 = X[nrow(X) - n*9 + 1 : nrow(X) - n*8,  ]
